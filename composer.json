{
    "name": "dantleech/phpactor",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dantleech/class-mover"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dantleech/source-code-filesystem"
        }
    ],
    "require": {
        "symfony/console": "~2.7|^3.0",
        "phpbench/container": "^1.0",
        "dantleech/class-mover": "dev-master",
        "dantleech/source-code-filesystem": "dev-master",
        "symfony/filesystem": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "symfony/process": "^2.7|^3.0",
        "symfony/options-resolver": "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Tests\\": "tests/"
        }
    }
}
