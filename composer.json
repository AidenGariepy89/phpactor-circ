    "require": {
        "symfony/console": "~2.7",
        "symfony/finder": "~2.7",
        "symfony/process": "~2.7",
        "doctrine/dbal": "^2.5",
        "roave/better-reflection": "dev-variable_collector",
        "phpbench/container": "^1.0"
    },
